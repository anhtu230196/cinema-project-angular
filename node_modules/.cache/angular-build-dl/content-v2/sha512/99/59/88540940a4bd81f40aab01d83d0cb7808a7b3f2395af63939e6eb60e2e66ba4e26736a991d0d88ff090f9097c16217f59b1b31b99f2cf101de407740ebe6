        this.moviesService = moviesService;
        this.movieList = null;
    }
    // Tương đương componentDidMount bên react
    ngOnInit() {
        this.moviesService.getMovieList().subscribe({
            next: (result) => {
                this.movieList = result;
            },
            error: (error) => {
                console.log(error);
            },
            complete: () => console.log("Get movie list success"),
        });
    }
